{
	"name": "demo-credit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"start:dev": "nodemon ./bin/www",
		"migrate": "DB_NAME=demo_credit_lendsqr DB_USER=root DB_PASS=password knex migrate:latest --knexfile ./db/knexfile.js --knexpath ./db/dbconnect.js",
		"migrate-up": "DB_NAME=demo_credit_lendsqr DB_USER=root DB_PASS=password knex migrate:up --knexfile ./db/knexfile.js --knexpath ./db/dbconnect.js",
		"migrate-down": "DB_NAME=demo_credit_lendsqr DB_USER=root DB_PASS=password knex migrate:down --knexfile ./db/knexfile.js --knexpath ./db/dbconnect.js",
		"migrate-rollback": "DB_NAME=demo_credit_lendsqr DB_USER=root DB_PASS=password knex migrate:rollback --all --knexfile ./db/knexfile.js --knexpath ./db/dbconnect.js",
		"deploy": "git checkout -b prod && git merge main && git push"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^10.0.0",
		"express": "~4.16.1",
		"express-validator": "^6.12.2",
		"jsonwebtoken": "^8.5.1",
		"knex": "^2.3.0",
		"morgan": "~1.9.1",
		"mysql": "^2.18.1",
		"word-casing": "^1.0.6"
	},
	"engines": {
		"node": "16.16.0",
		"npm": "8.11.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.14"
	}
}
